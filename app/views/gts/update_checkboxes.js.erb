<% if params[:box] == "under" %>
    <% @gt.toggle!(:under_bool) %>
    <% if @gt.under_bool %>
        $('#table_under-<%= @gt.id %>').html('<i class="text-success far fa-check-square fa-lg"></i>')
    <% else %>
        $('#table_under-<%= @gt.id %>').html('<i class="text-danger far fa-square fa-lg"></i>')
    <% end %>
<% end %>
